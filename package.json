    "framer-motion": "^6.5.1",
    "next": "12.2.0",
    "next-seo": "^5.5.0",
    "preact": "^10.5.14",
    "preact-render-to-string": "^5.1.19",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.15.8",
    "@chakra-ui/styled-system": ">=2.0.0",
