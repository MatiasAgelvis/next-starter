    "@vitebook/markdown-preact": "^0.100.5",
    "@vitebook/preact": "^0.100.5",
    "@vitebook/theme-default": "^0.100.5",
    "commitizen": "^4.2.5",
    "commitlint": "^17.0.3",
    "cross-env": "^7.0.3",
    "eslint": "^8.21.0",
    "eslint-config-next": "^12.2.5",
    "eslint-config-sznm": "^1.0.0",
    "husky": "^8.0.1",
    "jsdom": "^20.0.0",
    "lint-staged": "^13.0.3",
    "next-sitemap": "^3.1.21",
    "prettier": "^2.7.1",
    "standard-version": "^9.5.0",
